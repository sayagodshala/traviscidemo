<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#FFF" />

            <stroke android:color="@color/grey2" android:width="0.5dp"/>
            <!--<corners android:radius="10dp" />-->
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <layer-list>
                <item>
                    <color android:color="@color/grey2" />
                </item>
                <item
                    android:bottom="1dp"
                    android:left="1dp"
                    android:right="1dp"
                    android:top="1dp">
                    <shape>
                        <solid android:color="@color/team_rcb" />
                    </shape>
                </item>
            </layer-list>
        </clip>
    </item>
</layer-list>